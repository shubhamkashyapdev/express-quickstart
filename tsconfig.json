{
  "compilerOptions": {
    "target": "ES5",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "./src",
    "outDir": "dist"
  },
  "include": [
    "./src/**/*.ts",
    "types.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}